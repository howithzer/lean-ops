{
    "name": "Test: Network Duplicates (SQS)",
    "description": "30% duplicate messageId to test RAW layer handles duplicates gracefully",
    "run_mode": "local",
    "log_format": "user",
    "log_level": "INFO",
    "sink_type": "sqs",
    "concurrency": 5,
    "batch_size": 50,
    "total_records": 500,
    "metrics_interval_seconds": 10,
    "schema_config": {
        "base_schema": "../../Testing_Framework_EEH_2.0/sample-data-generator/schemas/iceberg_raw_events_v2.json",
        "drift": {
            "enabled": false,
            "percentage": 0.0
        },
        "duplicates": {
            "enabled": true,
            "percentage": 0.3,
            "max_duplicates": 5,
            "duplicate_key": "messageId"
        }
    },
    "sqs_config": {
        "target_sqs_url": "https://sqs.us-east-1.amazonaws.com/487500748616/lean-ops-dev-events-queue",
        "region": "us-east-1"
    }
}